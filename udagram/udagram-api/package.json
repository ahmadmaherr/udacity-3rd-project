{
  "name": "udagram-api",
  "version": "2.0.0",
  "description": "",
  "engines": {
    "node": "14.15.0"
  },
  "main": "server.ts",
  "scripts": {
    "start": "node ./server.js",
    "tsc": "npx tsc",
    "dev": "npx ts-node-dev --respawn --transpile-only ./src/server.ts",
    "prod": "npx tsc && node ./server.js",
    "clean": "rm -rf www/ || true",
    "deploy": "npm run build && eb list && eb use udagram-api-dev && eb deploy && eb setenv PORT=$PORT DB_PORT=$DB_PORT POSTGRES_USERNAME=$POSTGRES_USERNAME POSTGRES_PASSWORD=$POSTGRES_PASSWORD POSTGRES_DB=$POSTGRES_DB POSTGRES_HOST=$POSTGRES_HOST RDS_DIALECT=$RDS_DIALECT AWS_REGION=$AWS_REGION JWT_SECRET=$JWT_SECRET",
    "build": "npm install . && npm run clean && tsc && cp -rf src/config www/config && cp -R .elasticbeanstalk www/.elasticbeanstalk && cp .npmrc www/.npmrc && cp package.json www/package.json && cd www && zip -r Archive.zip . && cd ..",
    "test": "jasmine-ts"
  },
  "keywords": [],
  "author": "Gabriel Ruttner",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.4",
    "aws-sdk": "^2.1222.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^16.4.3",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.31.0",
    "eslint-config-google": "^0.14.0",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "mocha": "^9.0.3",
    "supertest": "^6.2.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
